{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "my-component-library",
  "author": "kirbie27",
  "module": "dist/my-component-library.esm.js",
  "size-limit": [
    {
      "path": "dist/my-component-library.cjs.production.min.js",
      "limit": "10 KB",
      "preset": "@size-limit/preset-small-lib"
    },
    {
      "path": "dist/my-component-library.esm.js",
      "limit": "10 KB",
      "preset": "@size-limit/preset-small-lib"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "husky": "^9.1.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "size-limit": "^11.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^3.9.10"
  }
}
